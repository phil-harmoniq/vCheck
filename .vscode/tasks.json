{
	"type": "shell",
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Solution",
			"command": "dotnet",
			"args": [ "build" ],
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Restore Nuget Packages",
			"command": "dotnet",
			"args": [ "restore" ],
			"problemMatcher": "$msCompile",
			"group": "build"
		},
		{
			"label": "Run All Tests",
			"command": "dotnet",
			"args": [ "test", "vCheck.Tests" ],
			"problemMatcher": "$msCompile",
			"group": "build"
		}
	]
}